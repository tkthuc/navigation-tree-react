{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "React": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "jsx-quotes": [
      2,
      "prefer-single"
    ],
    "react/jsx-curly-spacing": [
      2,
      "always"
    ],
    "react/prefer-stateless-function": [
      0
    ],
    "linebreak-style" : [0],
    "no-unused-vars" : [0],
    "import/extensions" : [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}